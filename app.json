{
  "expo": {
    "name": "Trivory",
    "slug": "Trivory",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.terrenprogramming.riderappdev"
    },
    "android": {
      "package": "com.terrenprogramming.riderappdev"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "react-native-edge-to-edge",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "scheme": "trivory",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
